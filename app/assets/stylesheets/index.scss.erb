@media all and (max-width:1322px) {
  .intro-header {
  background: asset-url('imgtest/bg1.jpg') no-repeat 0 0 ;
  min-height: 681px;
  background-size: cover;
  }

}
@media all and (max-width:1199px) {
  .trapezium-right {
      border-left: 100px solid transparent;
      width: 40%;
  }
  .trapezium-left {
      border-right: 100px solid transparent;
      width: 40%;
  }
  .intro-header .sitetitle {
    margin: 0 40%;
    margin-bottom: 50px;

    // font-size: 5em;
  }

}
@media all and (max-width:991px) {
  .intro-header .sitetitle {
    margin: 0 25%;
    margin-bottom: 50px;

    // font-size: 5em;
  }
  .trapezium-right , .trapezium-left {
  display: none
  }
  .intro-message {
    padding: 0;
    margin-top: 70px;
  }
  .intro-message h1{
    background-color: rgba(255, 255, 255, 0.6);
    padding: 5% 0;
    border-radius: 20px;
    margin: 0;
    color: white;
    text-shadow: 2px 2px 3px rgba(0,0,0,0.9);
  }
  .intro-header {
    min-height: 200px;
    max-height: 381px;
  background: asset-url('imgtest/bg1.jpg') no-repeat 0 0 ;
  background-size: cover;
  padding-top: 120px;
  padding-bottom: 70px;
  }
  .home-search-bloc-resp{
    background-color: rgba(255, 255, 255, 0.84);
    padding: 10px;
    padding-top: 20px;
    width: 95%;
    margin-left: 2.4%
  }
  .home-search-bloc-resp .separator{
      height: 20px;
      margin-bottom: 20px;
      border-bottom: 3px solid white;
      margin-left: 15%;
      margin-right: 15%;
      width:70%
  }
  .home-mag ul li {
    margin-bottom: 20px
  }
  .home-post-bloc a {
    margin-top: 10px
  }
  .home-testimonial-bloc ul li{
  padding: 0
  }
  .home-testimonial-bloc ul li{
  padding: 30px 10px
  }
  .navbar-right{
    padding-left: 0;
    float: right;
    background-color: transparent !important
  }
  hr{
    display: inline-block !important
  }
}

@media all and (max-width:767px) {
  .navbar-right .droppeddown .dropdown-menu {
    position: relative;
    width: 0;
    height: 0;
    float: right;

  }

  .navbar-right .droppeddown .dropdown-menu, .navicouser,   .navbar-right .droppeddown{
      transition: 3s;
      transition-timing-function: cubic-bezier(.86,.02,.14,1);
      -webkit-transition: 3s;
    }
    .navbar-right .droppeddown.open .dropdown-menu {
      display: inline-block;
      width: 100%;
      height: auto;

    }

}
