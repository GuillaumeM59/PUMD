  background-color: rgba(255, 255, 255, 0.85);
  border: none
}
.navbar-brand {
  padding: 0
}

// --------------------------------------------------------
.propocontainer {
  padding-top: 80px;
}

.intro-header {
  position: relative;
background: asset-url('imgtest/bg1.jpg') no-repeat 0 -200px ;
background-size: cover;
  text-align: center;
  width: 100%;
  color: #ffffff;
overflow: hidden;
padding-top: 140px;
padding-bottom: 100px;
min-height: 681px
}

.intro-header, .trapezium-left, .trapezium-right{
    transition: 1.5s;
    transition-timing-function: cubic-bezier(.86,.02,.14,1);
    -webkit-transition: 1.5s;
  }
.trapezium-left {
  border-bottom: 722px solid rgba(201, 201, 201, 0.7);

    border-right: 200px solid transparent;
    height: 0;
    width: 40%;
    position: absolute;
    top: 81px;
    left: -100%;

}
.trapezium-left.clearbg{
  border-bottom: 722px solid transparent;
}
.trapezium-right.clearbg{
  border-bottom: 722px solid transparent;
}
.trapezium-left.active{
  left: 0;
}
.trapezium-right.active{
  right: 0;
}
.trapezium-right {
  border-bottom: 722px solid rgba(201, 201, 201, 0.7);

    border-left: 200px solid transparent;
    height: 0;
    width: 40%;
    position: absolute;
    top: 81px;
    right: -100%;

}
.trapezium-left .home-search-bloc{
  margin-left: 4%;
}
.trapezium-right .home-search-bloc{
  margin-left: 1%;
}
.etc{
	font-size: 1.5em;
	text-align: left;
	padding-top: 10px;
}
.offert{
	display: inline;
	vertical-align: bottom;
	padding-left: 10px;

}
#login-form .signuppurple, #register-form .signuppurple {
  margin-top: 0 !important;
  margin-bottom: 20px !important;
}

#register-form .signuppurple .data_input, #register-form .signuppurple .password_input, #login-form .signuppurple .data_input, #login-form .signuppurple .password_input {
  padding: 15px 0;
}

